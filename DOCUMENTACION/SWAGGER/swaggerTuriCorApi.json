        "idCiudad",
        "idPais",
        "id",
        "cliente",
        "vendedor"
      ],
      "properties": {
        "codigoReserva": {
          "type": "string",
          "example": "Reserva1"
        },
        "fechaReserva": {
          "type": "string",
          "example": "23/06/19"
        },
        "idCliente": {
          "type": "integer",
          "example": 69470
        },
        "idVendedor": {
          "type": "integer",
          "example": 69876
        },
