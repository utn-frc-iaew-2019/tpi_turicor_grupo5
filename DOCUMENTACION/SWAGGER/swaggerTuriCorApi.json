        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "idCliente",
            "in": "path",
            "description": "ID del cliente al cual se le asocian las reservas",
            "required": true,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "éxito",
            "schema": {
              "type": "array",
